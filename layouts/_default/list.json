{{- $.Scratch.Add "index" slice -}}
{{- range where .Site.RegularPages "Type" "materials" -}}
  {{- $.Scratch.Add "index" (dict 
    "objectID" .File.UniqueID 
    "title" .Title 
    "permalink" .Permalink
    "file_url" .Params.file_url
    "tags" .Params.tags 
    "summary" .Summary) -}}
{{- end -}}
{{- $.Scratch.Get "index" | jsonify -}}
